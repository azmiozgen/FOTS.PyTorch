{
    "name": "FOTS",
    "cuda": true,
    "gpus": [0],
    "data_loader": {
        "dataset":"price_tag",
        "data_dirname": "price_tag_small",
        "input_size": 256,
        "batch_size": 8,
        "shuffle": true,
        "workers": 8
    },
    "validation": {
        "validation_split": 0,
        "shuffle": true
    },
    "lr_scheduler_type": "",
    "lr_scheduler_freq": 10000,
    "lr_scheduler": {
            "gamma": 0.94
    },
 
    "optimizer_type": "AdamW",
    "optimizer": {
        "lr" : 0.0001,
        "betas" : [0.9, 0.999],
        "eps" : 1e-8,
        "weight_decay" : 0.01,
        "amsgrad" : false
    },
    "loss": "FOTSLoss",
    "metrics": ["fots_metric"],
    "trainer": {
        "epochs": 300,
        "save_dir": "saved/",
        "save_freq": 50,
        "verbosity": 2,
        "monitor": "hmean",
        "monitor_mode": "max"
    },
    "arch": "FOTSModel",
    "model": {
        "mode": "united",
        "score_map_threshold" : 0.5,
        "dropout" : 0.8
    }
}
